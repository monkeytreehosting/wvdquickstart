{
    "onedrive": [
        {
            "installOneDrive": true,
            "configureOneDrive": true,
            "profileContainerKeys": [
                # to be edited for OneDrive RegEdits
                {
                    "Name": "Enabled",
                    "Type": "DWORD",
                    "Value": "1"
                },
                {
                    "Name": "VHDLocations",
                    "Type": "MultiString",
                    "Value": "\\\\[profilesStorageAccountName].file.core.windows.net\\[profilesShareName]"
                }
            ],
            "fileShareName": "[profilesShareName]",
            "fileShareStorageAccountName": "[profilesStorageAccountName]",
            "domain": "[domainName]",
            "targetGroup": "[targetGroup]"
        }
    ]
}
